@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ex: <http://example.com/ns#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

<#MovieMapping>
	rml:logicalSource [
		rml:source 	"http://www.example.com/movies.xml" ;
		rml:iterator 	"/movielist/movie";
		rml:referenceFormulation 	ql:XPath;
	];

	rr:subjectMap [ 
		rr:template "http://movie.example.com/title/{/title}";
		rr:class ex:Movie
	];

	rr:predicateObjectMap [
		rr:predicate	ex:Genre;
		rr:objectMap [
			rml:template 	"ex:{/movielist/movie/genres/genre}"
		] 
	];
	rr:predicateObjectMap [
		rr:predicate	ex:ReleaseYear;
		rr:objectMap [
			rml:reference 	"/releaseYear";
			rr:datatype	xsd:int
		] 
	];
	rr:predicateObjectMap [
		rr:predicate	ex:Director;
		rr:objectMap [
			rml:template 	"http://movie.example.com/title/director/{/movielist/movie/Director}"
		] 
	].
